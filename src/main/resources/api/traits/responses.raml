#%RAML 1.0 Trait
responses:
  400:
    description: BAD REQUEST
    body:
      application/json:
        example: {
          message : "Incorrect Data. Please Check."
        }
  404:
    description: NOT FOUND
    body:
      application/json:
        example: {
          message : "Resource Not Found."
        }
  500:
    description: SERVER ERROR
    body:
      application/json:
        example: {
          message : "Unknown Error."
        }